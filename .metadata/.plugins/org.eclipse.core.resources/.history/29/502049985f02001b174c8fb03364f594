public class RGBtoCMYK {

    public static void main(String[] args) {
    int red = Integer.parseInt(args[0]); // create variable, allocate space and assign value
    int green = Integer.parseInt(args[1]); // creating the variable, allocate space, assign in 1 step
    int blue = Integer.parseInt(args[2]);
    float white = Math.max(Math.max(green/255, red/255), blue/255); 
   System.out.println(white);
    }
}